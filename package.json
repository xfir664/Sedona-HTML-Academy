{
  "name": "vite-react-js-template",
  "homepage": "https://xfir664.github.io/Sedona-HTML-Academy/",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Edge versions",
    "last 2 Firefox versions",
    "last 2 Safari versions"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && rimraf dist/img/icons",
    "lint": "eslint .",
    "preview": "vite preview",
    "optimize:raster": "node scripts/sharp.js",
    "optimize:vector": "node scripts/svgo.js",
    "optimize:all": "node scripts/svgo.js && node scripts/sharp.js",
    "create-sprite": "node scripts/sprite.js && svg-sprite --config sprite.config.json public/img/icons/*.svg",
    "fonts": "node scripts/convert-fonts.js",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@spiriit/vite-plugin-svg-spritemap": "^4.1.0",
    "@types/node": "^22.15.30",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "axios": "^1.7.9",
    "cheerio": "^1.1.0",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "fs-extra": "^11.3.0",
    "gh-pages": "^6.3.0",
    "globals": "^15.14.0",
    "localforage": "^1.10.0",
    "msw": "^2.9.0",
    "nanoid": "^5.1.5",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.4",
    "prop-types": "^15.8.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.1.5",
    "react-uuid": "^2.0.0",
    "rimraf": "^6.0.1",
    "sass": "^1.84.0",
    "sharp": "^0.33.5",
    "styled-components": "^6.1.15",
    "svg-sprite": "^2.0.4",
    "svgo": "^3.3.2",
    "swiper": "^11.2.3",
    "ttf2woff": "^3.0.0",
    "ttf2woff2": "^6.0.1",
    "typescript": "^5.8.3",
    "vite": "^6.1.0",
    "vite-plugin-checker": "^0.9.3",
    "vite-plugin-svgo": "^1.6.2"
  }
}
